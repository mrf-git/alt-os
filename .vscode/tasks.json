{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Tool: codegen",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/workspace/tools/codegen",
                "${workspaceFolder}/tools/codegen"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "go"
        },
        {
            "label": "Run Tool: codegen -p (generate protos)",
            "type": "process",
            "args": [
                "-p"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "${workspaceFolder}/workspace/tools/codegen",
            "problemMatcher": []
        },
        {
            "label": "Build Exe: ct-runtime",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/workspace/exe/ct-runtime",
                "alt-os/exe/ct-runtime"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "go"
        },
        {
            "label": "Run Exe: ct-runtime (os)",
            "type": "process",
            "args": [
                "-i",
                "${workspaceFolder}/def/os/container/runtime/os/api-run.yml"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "${workspaceFolder}/workspace/exe/ct-runtime",
            "problemMatcher": []
        },
        {
            "label": "Build Exe: ct-bundle",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/workspace/exe/ct-bundle",
                "alt-os/exe/ct-bundle"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "go"
        },
        {
            "label": "Run Exe: ct-bundle (os)",
            "type": "process",
            "args": [
                "-i",
                "${workspaceFolder}/def/os/container/bundle/os/api-create.yml"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "command": "${workspaceFolder}/workspace/exe/ct-bundle",
            "problemMatcher": []
        }
    ]
}
